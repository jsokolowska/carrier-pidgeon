<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="controller.ContactsController" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar fx:id="menuBar"  VBox.vgrow="NEVER">
      <menus>
        <Menu id="goMenu" mnemonicParsing="false" text="General">
          <items>
              <MenuItem fx:id="menuHome" onAction="#goHome" mnemonicParsing="false" text="Home" />
              <SeparatorMenuItem mnemonicParsing="false" />
              <MenuItem fx:id="menuLogOut" onAction="#logOut" mnemonicParsing="false" text="Log out" />
              <MenuItem fx:id="menuQuit" onAction="#quit" mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <HBox prefHeight="39.0" prefWidth="640.0">
         <children>
            <Label id="yourConnectionsLabel" alignment="CENTER" prefHeight="23.0" prefWidth="435.0" text="Contacts">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="newConnectionButton" onAction="#connect" alignment="TOP_LEFT" mnemonicParsing="false" prefHeight="26.0" prefWidth="133.0" text="Add new contact">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ScrollPane prefHeight="325.0" prefWidth="640.0">
         <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="539.0" prefWidth="621.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" />
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
  </children>
</VBox>
