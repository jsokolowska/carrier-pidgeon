<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu id="goMenu" mnemonicParsing="false" text="Go">
          <items>
            <MenuItem id="menuHome" mnemonicParsing="false" text="Home" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem id="menuQuit" mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <HBox prefHeight="39.0" prefWidth="640.0">
         <children>
            <Label alignment="CENTER" prefHeight="23.0" prefWidth="435.0" text="Your connections">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Button id="newConnectionButton" alignment="TOP_LEFT" mnemonicParsing="false" prefHeight="40.0" prefWidth="151.0" text="New connection">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ScrollPane prefHeight="325.0" prefWidth="640.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="539.0" prefWidth="621.0">
               <children>
                  <VBox id="MessageBox" prefHeight="539.0" prefWidth="621.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children></AnchorPane>
        </content>
      </ScrollPane>
  </children>
